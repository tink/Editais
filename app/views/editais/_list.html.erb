<ul id="lista">
  <% local_assigns[:editais].each do |edital| %>
    <li <%= (edital == local_assigns[:editais].last) ? 'class=last' : '' %>>
      <h2><%= link_to edital.nome, edital %></h2>
      <p class="date"><%= l edital.data_publicacao %> at√© <%= l edital.data_limite %> - <%= links_to_tags edital.tags %></p>
      <p class="resumo"><%= edital.resumo %></p>
    </li>
  <% end %>
</ul>
